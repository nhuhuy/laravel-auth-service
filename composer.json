{
    "name": "nhuhuy/auth-service",
    "description": "laravel-auth-service",
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyen Nhu Huy",
            "email": "nhuhuy25@gmail.com",
            "homepage": "https://github.com/nhuhuy"
        }
    ],
    "homepage": "https://github.com/nhuhuy/authservice",
    "keywords": ["Laravel", "AuthService"],
    "require": {
        "illuminate/support": "~5|~6",
        "spatie/data-transfer-object": "^1.9",
        "laravel/passport": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "nhuhuy\\AuthService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "nhuhuy\\AuthService\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "nhuhuy\\AuthService\\AuthServiceServiceProvider"
            ],
            "aliases": {
                "AuthService": "nhuhuy\\AuthService\\Facades\\AuthService"
            }
        }
    },
    "minimum-stability": "dev"
}
